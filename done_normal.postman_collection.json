		{
			"name": "Person identity endpoint",
			"event": [
				{
					"listen": "test",
					"script": {
						"type": "text/javascript",
						"exec": [
							"tests[\"Status code is 200\"] = responseCode.code === 200;",
							"",
							"var response = JSON.parse(responseBody);",
							"",
							"if (response && response[\"billing-accounts\"] && response[\"billing-accounts\"].length && response[\"billing-accounts\"].length > 0) {",
							"    var ba = response[\"billing-accounts\"][0];",
							"    if (ba[\"mobile-subscriptions\"] && ba[\"mobile-subscriptions\"].length && ba[\"mobile-subscriptions\"].length > 0) {",
							"        var s = ba[\"mobile-subscriptions\"][0];",
							"        if (s.links && s.links.length && s.links.length > 0) {",
							"            for (var i = 0; i < s.links.length; i++) {",
							"                var link = s.links[i];",
							"                if (link.rel === \"inlife-recommendations\") {",
							"                    postman.setEnvironmentVariable(\"irechref\", link.href);",
							"                }",
							"            }",
							"        }",
							"    }",
							"}"
						]
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"type": "text/javascript",
						"exec": [
							"postman.setEnvironmentVariable(",
							"    \"trackingHeader\", ",
							"    'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) { ",
							"        var r = Math.random()*16|0, v = c == 'x' ? r : (r&0x3|0x8); return v.toString(16); ",
							"    })",
							");"
						]
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "X-EE-EL-Tracking-Header",
						"value": "{{trackingHeader}}"
					},
					{
						"key": "X-EE-API-Originator",
						"value": "Originator121"
					},
					{
						"key": "Authorization",
						"value": "Bearer {{accessToken}}"
					}
				],
				"body": {
					"mode": "raw",
					"raw": ""
				},
				"url": {
					"raw": "https://api-test1.ee.co.uk/digital-inlife/v1/person-identities/self",
					"protocol": "https",
					"host": [
						"api-test1",
						"ee",
						"co",
						"uk"
					],
					"path": [
						"digital-inlife",
						"v1",
						"person-identities",
						"self"
					]
				}
			},
			"response": []
		},
